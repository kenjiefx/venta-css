{
    "item-width": {
		"rule": "width:{value}px",
        "type": "minmax",
		"values": ["120","240"],
		"variants": "24",
		"separator": "-"
	},
    "position": {
        "rule": "position:{value}",
        "type": "list",
        "values": ["inherit","fixed","relative","static","absolute"],
        "variants":"auto",
        "separator":"-"
    }
}