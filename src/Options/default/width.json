{
    "width": {
      "rule": "width:{value}%",
      "type": "minmax",
      "values": [0, 100]
    },
    "width-static": {
      "rule": "width:{value}",
      "type": "list",
      "values": [
        "max-content",
        "min-content",
        "fit-content",
        "auto",
        "inherit",
        "initial",
        "unset"
      ]
    },
    "max-width-static": {
      "rule": "max-width:{value}",
      "type": "list",
      "values": [
        "max-content",
        "min-content",
        "fit-content",
        "none",
        "auto",
        "inherit",
        "initial",
        "unset"
      ]
    },
    "min-width-static": {
      "rule": "min-width:{value}",
      "type": "list",
      "values": [
        "max-content",
        "min-content",
        "fit-content",
        "auto",
        "inherit",
        "initial",
        "unset"
      ]
    }
  }
  